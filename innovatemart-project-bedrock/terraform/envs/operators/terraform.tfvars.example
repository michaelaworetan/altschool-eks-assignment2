# Example terraform.tfvars for operators environment
# Copy this file to terraform.tfvars and update with your values

# Route53 Configuration (for custom domains with SSL)
route53_zone_id = "Z1234567890ABC"  # Your Route53 Zone ID
ingress_hostname = "shop.yourdomain.com"  # Your custom domain

# ALB Ingress Management
manage_ui_ingress = true

# Optional: Override sandbox state location if different
# sandbox_state_bucket = "your-custom-bucket-name"
# sandbox_state_key = "custom/path/terraform.tfstate"
# sandbox_state_region = "eu-west-1"